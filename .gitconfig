[user]
	name = Alexander Nestorov
	email = alexandernst@gmail.com
[push]
	default = current
[core]
	filemode = false
	compression = 9
	editor = nano
[color]
	autocrlf = true
	branch = auto
	diff = auto
	grep = auto
	interactive = auto
	status = auto
	ui = auto
[diff]
	context = 6
	renames = true
[grep]
	lineNumber = true
[pack]
	compression = 9
[rebase]
	autostash = true
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue) <%an>%Creset' --abbrev-commit
